{
  "name": "@aponia.js/core",
  "version": "0.0.1",
  "description": "aponia.js core - a web authentication framework",
  "license": "MIT",
  "homepage": "https://elysium-everlasting.github.io/aponia.js",
  "funding": "https://github.com/Elysium-Everlasting/aponia.js",
  "contributors": [
    {
      "name": "ap0nia",
      "email": "",
      "url": "https://github.com/ap0nia"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Elysium-Everlasting/aponia.js",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/Elysium-Everlasting/aponia.js/issues"
  },
  "keywords": [],
  "type": "module",
  "types": "./src/index.ts",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./src/controllers/client-auth.ts",
      "require": "./src/controllers/client-auth.ts",
      "default": "./src/controllers/client-auth.ts",
      "import": "./src/controllers/client-auth.ts"
    },
    "./session": {
      "types": "./src/controllers/session.ts",
      "require": "./src/controllers/session.ts",
      "default": "./src/controllers/session.ts",
      "import": "./src/controllers/session.ts"
    },
    "./providers/credentials": {
      "types": "./src/providers/credentials.ts",
      "require": "./src/providers/credentials.ts",
      "default": "./src/providers/credentials.js",
      "import": "./src/providers/credentials.js"
    },
    "./providers/email": {
      "types": "./src/providers/email.ts",
      "require": "./src/providers/email.ts",
      "default": "./src/providers/email.ts",
      "import": "./src/providers/email.ts"
    },
    "./providers/*": {
      "types": "./providers/*.ts",
      "require": "./providers/*.ts",
      "default": "./providers/*.ts",
      "import": "./providers/*.ts"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "src/index.ts"
      ],
      "client": [
        "./src/controllers/client-auth.ts"
      ],
      "session": [
        "./src/controllers/session.ts"
      ],
      "providers/credentials": [
        "./src/providers/credentials.ts"
      ],
      "providers/email": [
        "./src/providers/email.ts"
      ],
      "providers/*": [
        "./providers/*.ts"
      ]
    }
  },
  "scripts": {
    "format": "prettier --write --cache .",
    "lint": "eslint --cache .",
    "build": "tsup",
    "test": "vitest"
  },
  "dependencies": {
    "@panva/hkdf": "^1.1.1",
    "cookie": "^0.5.0",
    "defu": "^6.1.3",
    "jose": "^4.14.4",
    "oauth4webapi": "^2.3.0",
    "universal-cookie": "^6.1.1"
  },
  "devDependencies": {
    "@auth/core": "^0.18.1",
    "@types/cookie": "^0.5.1",
    "@types/universal-cookie": "^3.0.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "packageManager": "pnpm@8.6.12",
  "engines": {
    "pnpm": "^8.0.0",
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
