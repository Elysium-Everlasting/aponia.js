{
  "name": "@aponia.js/core",
  "version": "0.0.0",
  "description": "aponia.js core - a web authentication framework",
  "private": true,
  "license": "MIT",
  "homepage": "https://elysium-everlasting.github.io/aponia.js",
  "funding": "https://github.com/Elysium-Everlasting/aponia.js",
  "contributors": [
    {
      "name": "ap0nia",
      "email": "",
      "url": "https://github.com/ap0nia"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Elysium-Everlasting/aponia.js",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/Elysium-Everlasting/aponia.js/issues"
  },
  "keywords": [],
  "type": "module",
  ".": {
    "types": "./dist/src/index.d.ts",
    "require": "./dist/src/index.cjs",
    "default": "./dist/src/index.js"
  },
  "./session": {
    "types": "./dist/src/session.d.ts",
    "require": "./dist/src/session.cjs",
    "default": "./dist/src/session.js"
  },
  "./providers/credentials": {
    "types": "./dist/src/providers/credentials.d.ts",
    "require": "./dist/src/providers/credentials.cjs",
    "default": "./dist/src/providers/credentials.js"
  },
  "./providers/email": {
    "types": "./dist/src/providers/email.d.ts",
    "require": "./dist/src/providers/email.cjs",
    "default": "./dist/src/providers/email.js"
  },
  "./providers/*": {
    "types": "./dist/providers/*.d.ts",
    "require": "./dist/providers/*.cjs",
    "default": "./dist/providers/*.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/src/index.d.ts"
      ],
      "session": [
        "./dist/src/session.d.ts"
      ],
      "providers/credentials": [
        "./dist/src/providers/credentials.d.ts"
      ],
      "providers/email": [
        "./dist/src/providers/email.d.ts"
      ],
      "providers/*": [
        "./dist/providers/*.d.ts"
      ]
    }
  },
  "scripts": {
    "format": "prettier --write --cache .",
    "lint": "eslint --cache .",
    "build": "tsup",
    "test": "vitest"
  },
  "dependencies": {
    "@panva/hkdf": "^1.1.1",
    "cookie": "^0.5.0",
    "jose": "^4.14.4",
    "oauth4webapi": "^2.3.0"
  },
  "devDependencies": {
    "@types/cookie": "^0.5.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@8.6.12",
  "engines": {
    "pnpm": "^8.0.0",
    "node": ">=18"
  }
}
